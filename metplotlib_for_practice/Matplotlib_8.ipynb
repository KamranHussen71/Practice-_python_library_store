   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = [1,2,3,4,5]\n",
    "y = [2,2,3,5,4]\n",
    "\n",
    "plt.step(x,y,color=\"r\",marker=\"o\",ms=10,mfc=\"g\",label=\"Python\")\n",
    "\n",
    "plt.title(\"Python\")\n",
    "plt.xlabel(\"x - axis\")\n",
    "plt.ylabel(\"y - axis\")\n",
    "plt.legend(loc=2)\n",
    "plt.grid()\n",
    "plt.show()"
   ]
  }
